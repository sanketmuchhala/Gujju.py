{
  "name": "kemlang-vscode",
  "displayName": "KemLang Support",
  "description": "Syntax highlighting and language support for KemLang",
  "version": "0.1.0",
  "publisher": "kemlang",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "kemlang",
    "gujarati",
    "programming language",
    "syntax highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kemlang",
        "aliases": [
          "KemLang",
          "kemlang"
        ],
        "extensions": [
          ".kem"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kemlang",
        "scopeName": "source.kemlang",
        "path": "./syntaxes/kemlang.tmLanguage.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kemlang/kemlang"
  },
  "license": "MIT"
}